<div class="d-flex justify-content-center align-items-center pt-5">
  {{#each userList.recipeSearch}}

    <button type="button" class="btn btn-secondary btn-sm rounded-pill btnTag pl-3">{{this}}</button>
  
  {{/each}}
</div>




<div class="recipe-search container pt-2 px-0">
  <h1 class="ml-3">Recipes that match your search</h1>
  <h4 class="grey mb-5 ml-3">The percentage on each recipe represent how much you already have to cook it</h4>
  <div class="row d-flex justify-content-between ml-3">
{{#each recipes}}

{{>recipelist}}


{{/each}}
</div>
</div>
 